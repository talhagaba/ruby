



<%= link_to("BACK TO LIST", products_path, :class => 'back-link')%>
<div class="product delete">

	<h1>Admin Panel</h1>
<br>
  <h2>Delete Product</h2>
  <%= form_for(@product,:url => product_path(@product),:method => 'delete')do |f| %>
  <p>Are you sure you want to delete this product </p>
  <p class="reference-name"><%= @product.productName%></p>


  <div class="form-buttons">
    <%= f.submit("Delete product")%>
  </div>
  <%end%>
</div>
